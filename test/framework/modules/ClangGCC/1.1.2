}

if { ![is-loaded GCC/6.4.0-2.28] } {
    module load GCC/6.4.0-2.28
}


setenv	EBROOTCLANGGCC		"$root"
setenv	EBVERSIONCLANGGCC		"1.1.2"
setenv	EBDEVELCLANGGCC		"$root/easybuild/ClangGCC-1.1.2-easybuild-devel"


