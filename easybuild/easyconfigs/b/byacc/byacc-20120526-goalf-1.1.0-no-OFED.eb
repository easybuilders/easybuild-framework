name = 'byacc'
version = '20120526'

homepage = 'http://invisible-island.net/byacc/byacc.html'
description = """Berkeley Yacc (byacc) is generally conceded to be the best yacc variant available.
In contrast to bison, it is written to avoid dependencies upon a particular compiler."""

toolkit = {'name': 'goalf', 'version': '1.1.0-no-OFED'}

sources = ['%s-%s.tgz' % (name.lower(), version)]
sourceURLs = ['ftp://invisible-island.net/byacc']


sanityCheckPaths = {
                    'files': ["bin/yacc"],
                    'dirs': []
                   }
